-- ===== K·∫æT N·ªêI API T·ª™NG TAB =====
local endpoints = {
    ["full"] = "fullmoon",
    ["mir"] = "Mirage",
    ["sword"] = "swordlegend",
    ["dough"] = "Gay",
    ["rip"] = "Rip",
    ["dark"] = "Dark"
}

-- G·ªçi h√†m:
for short, real in pairs(endpoints) do
    local tab = Tabs["Servers" .. ({full=1, mir=2, sword=3, dough=4, rip=5, dark=6})[short]]
    CreateServerUpdater(tab, "http://greenzapi.serveirc.com:31447/Api/" .. real)
end

