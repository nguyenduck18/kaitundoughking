-- âœ… Äáº£m báº£o game Ä‘Ã£ load xong
repeat wait() until game:IsLoaded() and game.Players.LocalPlayer

-- ğŸ”‘ Láº¥y key vÃ  hwid tá»« getgenv() hoáº·c user ID Roblox
local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

local key = getgenv().Key or ""
local hwid = gethwid and gethwid() or tostring(Players.LocalPlayer.UserId)

-- ğŸŒ Domain Ä‘á»ƒ trá»±c tiáº¿p â€“ khÃ´ng encode
local domain = "shopgiare.cfd"
local url = "https://" .. domain .. "/api/checkhwid.php?key=" .. key .. "&hwid=" .. hwid

-- ğŸŒ Gá»­i request xÃ¡c thá»±c key
local success, response = pcall(function()
    return HttpService:JSONDecode(game:HttpGet(url))
end)

-- âŒ Náº¿u lá»—i thÃ¬ kick
if not success then
    Players.LocalPlayer:Kick("KhÃ´ng thá»ƒ káº¿t ná»‘i tá»›i server. LiÃªn há»‡ admin.")
    return
end

-- âŒ Náº¿u key khÃ´ng há»£p lá»‡ thÃ¬ cÅ©ng kick
if response.status ~= "true" then
    Players.LocalPlayer:Kick("Key hoáº·c HWID khÃ´ng há»£p lá»‡.")
    return
end

-- âœ… Náº¿u key há»£p lá»‡
print("âœ” Key há»£p lá»‡, script tiáº¿p tá»¥c cháº¡y...")

