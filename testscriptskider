    end
)
Tabs.Main:Toggle({
    Title = "Auto Lock Moon",
    Default = false,
    Callback = function(v36)
        _G.LockCamToMoon = v36
    end
})
spawn(
    function()
        while wait() do
            pcall(
                function()
                    if _G.LockCamToMoon then
                        wait(0.1)
                        local v83 = game.Lighting:GetMoonDirection()
                        local v84 = game.Workspace.CurrentCamera.CFrame.p + (v83 * 100)
                        game.Workspace.CurrentCamera.CFrame = CFrame.lookAt(game.Workspace.CurrentCamera.CFrame.p, v84)
                    end
                end
            )
        end
    end
)
Tabs.Main:Toggle({
    Title = "Tween Gear",
    Default = false,
    Callback = function(v37)
        _G.TweenMGear = v37
        StopTween(_G.TweenMGear)
    end
})
spawn(
    function()
        pcall(
            function()
                while wait() do
                    if _G.TweenMGear then
                        if game:GetService("Workspace").Map:FindFirstChild("MysticIsland") then
                            for v120, v121 in pairs(game:GetService("Workspace").Map.MysticIsland:GetChildren()) do
                                if v121:IsA("MeshPart") then
                                    if (v121.Material == Enum.Material.Neon) then
                                        topos(v121.CFrame)
                                    end
                                end
                            end
                        end
                    end
                end
            end
        )
    end
)
-- ===== T·∫†O H√ÄM C·∫¨P NH·∫¨T SERVER CHUNG =====
local function CreateServerUpdater(tab, apiUrl)
    local serverButtons = {}

    local function UpdateServerList()
        local success, response = pcall(function()
            return game:HttpGet(apiUrl)
        end)

        if not success then
            warn("Kh√¥ng th·ªÉ k·∫øt n·ªëi ƒë·∫øn API")
            return
        end

        local data
        local ok = pcall(function()
            data = HttpService:JSONDecode(response)
        end)

        if not ok or not data or not data.JobId then
            warn("L·ªói d·ªØ li·ªáu JSON ho·∫∑c thi·∫øu 'JobId'")
            warn("Ph·∫£n h·ªìi API:", response)
            return
        end

        -- Danh s√°ch ID m·ªõi
        local newIds = {}
        for _, entry in ipairs(data.JobId) do
            for id, _ in pairs(entry) do
                table.insert(newIds, id)
            end
        end

        -- So s√°nh v√† c·∫≠p nh·∫≠t
        local newSet = {}
        for _, id in ipairs(newIds) do newSet[id] = true end

        -- X√≥a n√∫t kh√¥ng c√≤n
        for id, button in pairs(serverButtons) do
            if not newSet[id] then
                button:Destroy()
                serverButtons[id] = nil
            end
        end

        -- T·∫°o n√∫t m·ªõi
        for _, id in ipairs(newIds) do
            if not serverButtons[id] then
                serverButtons[id] = tab:Button({
                    Title = "Join Server: " .. id,
                    Desc = "Click ƒë·ªÉ v√†o server",
                    Callback = function()
                        TeleportService:TeleportToPlaceInstance(PLACE_ID, id, Players.LocalPlayer)
                    end
                })
            end
        end
    end

    -- T·ª± ƒë·ªông c·∫≠p nh·∫≠t m·ªói 5 gi√¢y
    task.spawn(function()
        while true do
            pcall(UpdateServerList)
            task.wait(5)
        end
    end)
end

