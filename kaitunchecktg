local function notify(text)
    pcall(function()
        game.StarterGui:SetCore("SendNotification", {
            Title = "Zush Hub",
            Text = text,
            Duration = 3
        })
    end)
end

spawn(function()
    local searching = false
    local notifiedBoss = false
    local notifiedNone = false

    while true do
        local found = false
        for _, v in pairs(workspace:GetChildren()) do
            if v.Name == "Dough King" then
                found = true
                break
            end
        end

        if found then
            statusLabel.Text = "âœ… Dough King Ä‘Ã£ xuáº¥t hiá»‡n"
            statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            if not notifiedBoss then
                notify("ÄÃ£ xuáº¥t hiá»‡n dough king")
                notifiedBoss = true
                notifiedNone = false
            end
            _G.FarmBoss = true
            searching = false
        else
            if not searching then
                searching = true
                _G.FarmBoss = false
                statusLabel.Text = "âŒ KhÃ´ng cÃ³ Dough King"
                statusLabel.TextColor3 = Color3.fromRGB(255, 85, 85)
                if not notifiedNone then
                    notify("KhÃ´ng tá»“n táº¡i trong server")
                    notifiedNone = true
                    notifiedBoss = false
                end
                wait(2)
                statusLabel.Text = "ðŸ”„ Äang tÃ¬m server khÃ¡c..."
                statusLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
                notify("Báº¯t Ä‘áº§u hop server")
            end
        end

        wait(2.5)
    end
end)
